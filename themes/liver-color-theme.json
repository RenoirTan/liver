    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7f7f7f",
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "meta.block variable.other",
                "support.variable.property.js"
            ],
            "settings": {
                "foreground": "#d7d7d7"
            }
        },
        {
            "name": "Builtin Variables",
            "scope": [
                "variable.other.constant",
                "variable.language",
                "constant.language",
                "support.variable.magic",
                "variable.legacy.builtin",
                "constant.other.boolean"
            ],
            "settings": {
                "foreground": "#ff1a1a",
            }
        },
        {
            "name": "Constant Variables",
            "scope": [
                "constant.other.caps.python",
                "support.constant",
            ],
            "settings": {
                "foreground": "#ff1a1a",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Inline Expression",
            "scope": [
                "constant.character.format.placeholder.other",
                "punctuation.definition.template-expression",
                "punctuation.section.embedded",
                "source.rust punctuation.definition.interpolation",
                "source.rust meta.interpolation"
            ],
            "settings": {
                "foreground": "#d37ff0",
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#e3e3e3"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ff1a1a"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#9554e8",
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#9554e8",
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator",
                "keyword.other.template",
                "keyword.other.substitution",
                "keyword.operator.assignment",
                "keyword.operator.type",
                "keyword.operator.range",
                "storage.type.function.arrow",
                "meta.tag",
                "source.js constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#ff1a1a",
            }
        },
        {
            "name": "Keyword Operator, Operator 2",
            "scope": [
                "keyword.operator.new",
                "keyword.operator.delete",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "keyword.operator.dereference",
                "keyword.operator.unpacking"
            ],
            "settings": {
                "foreground": "#9554e8"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "source.python punctuation",
                "punctuation.separator",
                "punctuation.section",
                "punctuation.accessor",
                "string.template meta.template.expression",
                "source.rust keyword.operator.key-value",
                "keyword.operator.namespace",
                "keyword.operator.access",
                "source.toml punctuation.definition.table.inline",
            ],
            "settings": {
                "foreground": "#d7d7d7",
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter",
                "punctuation.definition.tag",
                "punctuation.section.embedded",
                "punctuation.definition.decorator",
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js"
            ],
            "settings": {
                "foreground": "#ff6448"
            }
        },
        {
            "name": "Function, Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method",
                "entity.name.module.js",
                "variable.import.parameter.js",
                "variable.other.class.js",
                "entity.name.method.js",
                "source.python punctuation.separator.annotation.result",
                "source.rust keyword.operator.arrow.skinny"
            ],
            "settings": {
                "foreground": "#c390e8",
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "support.other.variable",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#eba2e8"
            }
        },
        {
            "name": "String",
            "scope": [
                "constant.character",
                "punctuation.definition.string",
                "string",
                "string.regexp",
            ],
            "settings": {
                "foreground": "#4fe399"
            }
        },
        {
            "name": "Escape Character",
            "scope": [
                "constant.character.escape",
                "constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#53e2ed",
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "keyword.other.unit",
                "constant.other.datetime",
                "constant.other.date"
            ],
            "settings": {
                "foreground": "#c390e8"
            }
        },
        {
            "name": "Parameter, Argument",
            "scope": [
                "variable.parameter",
                "meta.function-call.arguments"
            ],
            "settings": {
                "foreground": "#e3e3e3",
            }
        },
        {
            "name": "Constant Literal",
            "scope": [
                "constant.escape"
            ],
            "settings": {
                "foreground": "#53e2ed"
            }
        },
        {
            "name": "Symbols, Markup Heading",
            "scope": [
                "constant.other.symbol",
                "constant.other.key",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#4fe399"
            }
        },
        {
            "name": "Types, Class, Support, Imports",
            "scope": [
                "entity.name",
                "entity.other.inherited-class",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "meta.type meta.brace",
                "storage.modifier.import"
            ],
            "settings": {
                "foreground": "#c390e8",
            }
        },
        {
            "name": "Map Key",
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name",
                "source.postcss support.type.property-name",
                "meta.object-literal.key",
                "source.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#4fe399"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#c390e8"
            }
        },
        {
            "name": "Ini Table",
            "scope": [
                "source.ini entity.name",
                "source.toml punctuation.definition.table",
                "source.toml entity.other.attribute-name",
                "source.toml meta.tag"
            ],
            "settings": {
                "foreground": "#d37ff0"
            }
        },
        {
            "name": "Configuration File Key",
            "scope": [
                "source.json meta.structure.dictionary.json support.type.property-name.json",
                "punctuation.support.type.property-name",
                "source.toml entity.name.tag",
                "source.ini keyword.other",
                "text.html.markdown support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#fd3f92"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "text.html.basic entity.other.attribute-name.html",
                "text.html.basic entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ff7f00"
            }
        },
        {
            "name": "Sass Keyword Control",
            "scope": [
                "source.sass keyword.control"
            ],
            "settings": {
                "foreground": "#53e2ed"
            }
        },
        {
            "name": "Markup Inserted",
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#4fe399"
            }
        },
        {
            "name": "Markup Deleted",
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#ff6448"
            }
        },
        {
            "name": "Markup Changed",
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#c390e8"
            }
        },
        {
            "name": "URL",
            "scope": [
                "*url*",
                "*link*",
                "*uri*"
            ],
            "settings": {
                "foreground": "#53e2ed",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown Plain",
            "scope": [
                "text.html.markdown",
                "punctuation.definition.list_item.markdown"
            ],
            "settings": {
                "foreground": "#e3e3e3"
            }
        },
        {
            "name": "Markdown Markup Raw Inline",
            "scope": [
                "text.html.markdown markup.inline.raw.markdown"
            ],
            "settings": {
                "foreground": "#ff7f00"
            }
        },
        {
            "name": "Markdown Markup Raw Inline Punctuation",
            "scope": [
                "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
            ],
            "settings": {
                "foreground": "#ff7f00"
            }
        },
        {
            "name": "Markdown Heading",
            "scope": [
                "markdown.heading",
                "markup.heading | markup.heading entity.name",
                "markup.heading.markdown punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#eba2e8"
            }
        },
        {
            "name": "Markup Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#eba2e8"
            }
        },
        {
            "name": "Markup Bold",
            "scope": [
                "markup.bold",
                "markup.bold string"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#eba2e8"
            }
        },
        {
            "name": "Markup Bold-Italic",
            "scope": [
                "markup.bold markup.italic",
                "markup.italic markup.bold",
                "markup.quote markup.bold",
                "markup.bold markup.italic string",
                "markup.italic markup.bold string",
                "markup.quote markup.bold string"
            ],
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#eba2e8"
            }
        },
        {
            "name": "Markup Underline",
            "scope": [
                "markup.underline"
            ],
            "settings": {
