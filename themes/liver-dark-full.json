    "tokenColors": [
        {
            "name": "Brackets",
            "scope": [
                "meta.brace",
                "meta.parens",
                "punctuation.bracket",
                "punctuation.brackets",
                "punctuation.definition.arguments.begin",
                "punctuation.definition.arguments.end",
                "punctuation.definition.entity.begin",
                "punctuation.definition.entity.end",
                "punctuation.definition.type.begin",
                "punctuation.definition.type.end",
                "punctuation.section.scope.begin",
                "punctuation.section.scope.end",
                "punctuation.section.parens"
            ],
            "settings": {
                "foreground": "#d7d7d7"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable",
                "meta.function-call.arguments"
            ],
            "settings": {
                "foreground": "#d7d7d7"
            }
        },
        {
            "name": "Builtin",
            "scope": [
                "variable.language",
                "support.variable.magic",
                "variable.legacy.builtin",
                "variable.parameter.function.language.special"
            ],
            "settings": {
                "foreground": "#ff6448"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#ff1a1a"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "keyword.other",
                "keyword.other.unit",
                "keyword.operator.word",
                "keyword.operator.wordlike",
                "keyword.operator.new",
                "keyword.operator.delete",
                "keyword.operator.expression",
                "keyword.operator.noexcept",
                "storage"
            ],
            "settings": {
                "foreground": "#9554e8"
            }
        },
        {
            "name": "Logical Operator",
            "scope": [
                "keyword.operator.logical",
                "punctuation.separator.statement.and.shell"
            ],
            "settings": {
                "foreground": "#9554e8"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#ff1a1a"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation"
            ],
            "settings": {
                "foreground": "#d7d7d7"
            }
        },
        {
            "name": "Function, Method, Macro",
            "scope": [
                "entity.name.function",
                "meta.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#c390e8"
            }
        },
        {
            "name": "String",
            "scope": [
                "constant.character",
                "punctuation.definition.string",
                "punctuation.definition.regexp.begin",
                "punctuation.definition.regexp.end",
                "string"
            ],
            "settings": {
                "foreground": "#4fe399"
            }
        },
        {
            "name": "Escape Character",
            "scope": [
                "constant.character.escape",
                "constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#e4de41"
            }
        },
        {
            "name": "Format Sequence",
            "scope": [
                "constant.character.format.placeholder.other",
                "punctuation.definition.template-expression",
                "punctuation.definition.interpolation",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#e4de41"
            }
        },
        {
            "name": "Literals",
            "scope": [
                "constant.numeric",
                "constant.other.datetime",
                "constant.other.date",
                "constant.other.color.rgb-value.hex",
                "punctuation.definition.constant.css"
            ],
            "settings": {
                "foreground": "#eba2e8"
            }
        },
        {
            "name": "Types, Class, Support, Import, Namespace, Lifetimes",
            "scope": [
                "entity.name",
                "entity.other.inherited-class",
                "storage.modifier.import",
                "storage.type.generic",
                "entity.name.namespace",
                "entity.name.import",
                "meta.package.java",
                "meta.class",
                "meta.use",
                "storage.modifier.lifetime",
                "variable.other.enummember",
                "support.type",
                "support.class",
                "meta.package",
                "storage.modifier.package",
                "storage.type.java"
            ],
            "settings": {
                "foreground": "#eba2e8"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter",
                "punctuation.definition.tag",
                "punctuation.section.embedded",
                "tag"
            ],
            "settings": {
                "foreground": "#ff6448"
            }
        },
        {
            "name": "Config Key",
            "scope": [
                "support.type.property-name.json",
                "punctuation.support.type.property-name",
                "support.type.property-name",
                "source.toml entity.name.tag",
                "source.ini keyword.other",
                "text.html.markdown support.type.property-name.json",
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#fd3f92"
            }
        },
        {
            "name": "CSS Property",
            "scope": [
                "support.type.property-name.css"
            ],
            "settings": {
                "foreground": "#d7d7d7"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ff6448"
            }
        },
        {
            "name": "Ini Table",
            "scope": [
                "source.ini entity.name",
                "source.ini punctuation.definition",
                "source.toml punctuation.definition.table",
                "source.toml entity.other.attribute-name",
                "source.toml meta.tag"
            ],
            "settings": {
                "foreground": "#9554e8"
            }
        },
        {
            "name": "Markup Inserted",
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#4fe399"
            }
        },
        {
            "name": "Markup Deleted",
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#ff1a1a"
            }
        },
        {
            "name": "Markup Changed",
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#ffb370"
            }
        },
        {
            "name": "Markdown Plain",
            "scope": [
                "text.html.markdown",
                "punctuation.definition.list_item.markdown"
